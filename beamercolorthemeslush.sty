\setbeamercolor{title page}{fg=beamer@slush@white}
\setbeamercolor{separation line}{fg=beamer@slush@brightblack,bg=beamer@slush@brightblack}

\setbeamercolor{structure}{fg=beamer@slush@cyan}

\setbeamercolor{normal text}{fg=beamer@slush@brightwhite,bg=beamer@slush@black}
\setbeamercolor{alerted text}{fg=beamer@slush@brightred}
\setbeamercolor{example text}{fg=beamer@slush@cyan}

\setbeamercolor{block title}{fg=beamer@slush@cyan}
\setbeamercolor{block title alerted}{parent={alerted text,block title}}
\setbeamercolor{block title example}{parent={example text,block title}}

\setbeamercolor{block body}{parent=normal text}
\setbeamercolor{block body alerted}{parent={alerted text,block title},fg=alerted text.fg}
\setbeamercolor{block body example}{parent={example text,block title},fg=example text.fg}

\setbeamercolor{palette primary}{fg=beamer@slush@cyan}
\setbeamercolor{palette secondary}{fg=beamer@slush@brightcyan}
\setbeamercolor{palette tertiary}{fg=beamer@slush@brightblue}
\setbeamercolor{palette quaternary}{fg=beamer@slush@blue}

\setbeamercolor*{item}{fg=beamer@slush@brightblack}
\setbeamercolor*{subitem}{fg=beamer@slush@brightblack}
\setbeamercolor*{subsubitem}{fg=beamer@slush@brightblack}

\setbeamertemplate{itemize item}{\color{beamer@slush@brightblack}-}
\setbeamertemplate{itemize subitem}{\color{beamer@slush@brightblack}-}
\setbeamertemplate{itemize subsubitem}{\color{beamer@slush@brightblack}-}

